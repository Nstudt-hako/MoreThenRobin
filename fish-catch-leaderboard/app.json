{
  "expo": {
    "name": "Fish Catch Leaderboard",
    "slug": "fish-catch-leaderboard",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "theme": {
      "dark": true,
      "colors": {
        "primary": "#6200ee",
        "background": "#121212",
        "card": "#1f1f1f",
        "text": "#ffffff",
        "border": "#ffffff",
        "notification": "#f50057"
      }
    }
  }
}